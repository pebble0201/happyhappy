<java>
public class Solution {
    public static String solution(String rsp) {
        StringBuilder result = new StringBuilder();
        
        for (char ch : rsp.toCharArray()) {
            if (ch == '2') {
                result.append('0'); // ê°€ â†’ ë°”
            } else if (ch == '0') {
                result.append('5'); // ë°” â†’ ë³´
            } else if (ch == '5') {
                result.append('2'); // ë³´ â†’ ê°€
            }
        }
        
        return result.toString();
    }

<javascript>

function solution(rsp) {
    let result = ''; //ì´ˆê¸°í™”

    for (let ch of rsp) {
        if (ch == '2') {
            result = '0'; // ê°€ â†’ ë°”
        } else if (ch == '0') {
            result = '5'; // ë°” â†’ ë³´
        } else if (ch == '5') {
            result = '2'; // ë³´ â†’ ê°€
        }
    }

    return result;
}


